      "command": "yarn run projen",
      "category": 3
    },
    "bump": {
      "desc": "Commits a bump to the package version based on conventional commits",
      "command": "yarn run bump",
      "category": 2
    },
    "release": {
      "desc": "Bumps version & push to main",
      "command": "yarn run release",
      "category": 2
    },
    "test": {
      "desc": "Run tests",
      "command": "yarn run test",
      "category": 1
